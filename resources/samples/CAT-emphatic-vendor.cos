** Star Seed Timer
scrp 2 3 15 9
	addv ov01 1
	doif ov01 >= 10 and carr = null and fall = 0
		lock
		inst
		attr 16
		bhvr 0
		setv va00 0
		slow
		loop
			alph va00 1
			addv va00 4
		untl va00 >= 256
		setv va01 1
	endi
	doif va01 = 1
		kill ownr
	endi
endm
